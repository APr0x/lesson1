def get_matrix(n, m, value):
    matrix = []
    for i in range(n):   #n - rows
        matrix.append([])
        for j in range(m):   #m - colums
            matrix[i].append(value)
    return matrix



result1 = get_matrix(2, 4, 19)
result2 = get_matrix(1, 4, 69)
result3 = get_matrix(4,10,55)
print(result1)
print(result2)
